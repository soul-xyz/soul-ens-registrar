{
  "name": "soul-ens-registry",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:soul-xyz/ens-registry.git",
  "author": "<graemelearn@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "hardhat test",
    "generate:proofs": "ts-node merkle-distribution/generate-merkle-root.ts",
    "flatten:PermissionContract": "npx hardhat flatten contracts/PermissionContract.sol > flattened/PermissionContract.sol",
    "flatten:ENSResolver": "npx hardhat flatten contracts/ens/ENSResolver.sol > flattened/ENSResolver.sol",
    "verify:goerli": "hardhat verify --network goerli 0xDEB264E1F78FcD54A0a0aBbCd78F1898aDe26D26",
    "deploy:default": "hardhat run scripts/deploy.ts",
    "deploy:goerli": "hardhat run --network goerli scripts/deploy.ts",
    "deploy:mainnet": "hardhat run --network mainnet scripts/deploy.ts",
    "deploy:registrar:default": "hardhat run scripts/deploy-registrar.ts",
    "deploy:registrar:goerli": "hardhat run --network rinkeby scripts/deploy-registrar.ts"
  },
  "dependencies": {
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@openzeppelin/contracts": "^4.8.0",
    "@typechain/ethers-v5": "^5.0.0",
    "@types/mocha": "^8.2.0",
    "commander": "^9.4.1",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "chai": "^4.2.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.25",
    "hardhat": "^2.12.0",
    "hardhat-typechain": "^0.3.5",
    "solidity-coverage": "^0.8.2"
  }
}
